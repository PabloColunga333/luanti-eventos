{
  "headers": {
    "/_next/static/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/_next/image/**": {
      "Cache-Control": "public, max-age=86400, stale-while-revalidate=604800"
    },
    "/**/*.js": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.css": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.woff2": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.woff": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.ttf": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.png": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.jpg": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.jpeg": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.webp": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.svg": {
      "Cache-Control": "public, max-age=31536000, immutable"
